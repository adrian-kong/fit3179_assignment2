{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "data/drive.csv"
  },
  "repeat": {
    "row": [
      "Alt Ellips \\[m\\]",
      "Horiz Vel \\[m/s\\]",
      "SVs Used",
      "Corr\\. Age \\[s\\]"
    ]
  },
  "config": {
    "view": {
      "stroke": "transparent"
    },
    "background": null
  },
  "spec": {
    "height": 140,
    "width": 500,
    "layer": [
      {
        "params": [
          {
            "name": "brush",
            "select": {
              "type": "interval",
              "encodings": [
                "x"
              ]
            }
          }
        ],
        "mark": {
          "type": "line",
          "tooltip": true
        },
        "encoding": {
          "x": {
            "scale": {
              "zero": false
            },
            "title": "GPS Time of week [seconds]",
            "field": "GPS TOW \\[s\\]",
            "type": "quantitative"
          },
          "y": {
            "field": {
              "repeat": "row"
            },
            "type": "quantitative"
          },
          "color": {"value": "#ddd"}
        }
      },
      {
        "transform": [
          {
            "filter": {
              "param": "brush"
            }
          }
        ],
        "mark": "line",
        "encoding": {
          "x": {
            "scale": {
              "zero": false
            },
            "title": "GPS Time of week [seconds]",
            "field": "GPS TOW \\[s\\]",
            "type": "quantitative",
            "axis": {
              "grid": false
            }
          },
          "y": {
            "field": {
              "repeat": "row"
            },
            "type": "quantitative",
            "axis": {
              "grid": false
            }
          }
        }
      }
    ]
  }
}